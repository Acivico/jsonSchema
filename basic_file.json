{
	"id": "https://raw.githubusercontent.com/Acivico/jsonSchema/main/basic_file.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "VRE",
	"description": "",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string"
		},
		"expiration": {
			"type": "string",
			"format": "data-time"
		},
		"mtime": {
			"type": "string",
			"format": "data-time"
		},
		"owner": {
			"type": "string",
			"readOnly": true
		},
		"parentDir": {
			"type": "string"
		},
		"path": {
			"type": "string",
			"format": "path",
			"required": true			
		},
		"project": {
			"type": "string"
		},
		"size": {			
			"type": "integer"
		},
		"type": {
			"type": "string",
			"enum": [
				"file",
				"dir"
			]
		},
		"compressed": {
			"type": "string",
			"enum": [
				"gzip",
				"zip"
			]
		},
		"data_type": {
			"type": "string",
			"enum": [
				"structure",
				"tool_intermediate_file",
				"tool_statistics",
				"tss_classification_by_nucleosomes"
			],
			"choices":[
				["structure","Structure"],
				["tss_classification_by_nucleosomes","Classification nucleosomes"],
				["tool_intermediate_file","esta es la anterior a la ultima"],
				["tool_statistics","esta es otro ejemplo"]
			],
			"default": "configuration_file"
		},
		"description": {
			"type": "string"
		},
		"format": {
			"type": "string",
			"enum": [
				"Nifti"
			]
		},
		"input_files": {
			"type": "array",
			"uniqueItems": true		
		},
		"repository": {
			"type": "string"
		},
		"source_url": {
			"type": "string",
			"format": "uri"
		},
		"sources": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"validated": {
			"type": "string",
			"enum": [
				"TRUE",
				"FALSE"
			]
		},
		"visible": {
			"type": "boolean",
			"default": true
		},
		"cloudName": {
			"type": "string"
		}
	}
}
